<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myfootballmatch">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.myfootballmatch.ui.home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".success.SuccessRegistrationActivity" />
        <activity android:name=".login.registeredaccount.LoginWithRegisteredAccountActivity" />
        <activity android:name=".login.google.LoginWithGoogleActivity" />
        <activity android:name=".login.LoginActivity" />
        <activity android:name=".ui.league.PickLeagueRegisterActivity" />
        <activity android:name=".register.pickteamregister.PickTeamRegisterActivity" />
        <activity android:name=".register.RegisterActivity" />
        <activity android:name=".main.MainActivity"></activity>
    </application>

</manifest>