<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myfootballmatch">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.statistic.StatisticActivity"/>
        <activity android:name=".ui.squad.SquadActivity" />
        <activity android:name=".ui.team.register.TeamRegisterActivity" />
        <activity android:name=".ui.home.HomeActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.success.SuccessRegistrationActivity" />
        <activity android:name=".ui.login.registeredaccount.LoginWithRegisteredAccountActivity" />
        <activity android:name=".ui.login.google.LoginWithGoogleActivity" />
        <activity android:name=".ui.login.LoginActivity" />
        <activity android:name=".ui.league.PickLeagueRegisterActivity"/>
        <activity android:name=".ui.register.RegisterActivity" />
        <activity android:name=".ui.main.MainActivity" >

        </activity>
    </application>

</manifest>